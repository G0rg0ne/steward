[
  {
    "id": "redeploy",
    "execute-command": "/etc/webhook/redeploy.sh",
    "command-working-directory": "/etc/webhook",
    "response-message": "Redeploying flight agent...",
    "trigger-rule": {
      "match": {
        "type": "payload-hash-sha1",
        "secret": "{{.Env.WEBHOOK_SECRET}}",
        "parameter": {
          "source": "header",
          "name": "X-Hub-Signature"
        }
      }
    }
  }
] 